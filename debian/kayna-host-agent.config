#!/bin/sh
# config script for kayna-host-agent
#
# see: dh_installdeb(1)

set -e

# summary of how this script can be called:
#        * <config> `configure' <installed-version>
#        * <config> `reconfigure' <installed-version>
# for details, see http://www.debian.org/doc/debian-policy/ or
# the debian-policy package


case "$1" in
    configure|reconfigure)

    if [ -e /usr/share/debconf/confmodule ]; then
	. /usr/share/debconf/confmodule

	db_input high kayna-host-agent/docker_image_prefix || true

	db_go
    fi

    ;;

    *)
        echo "config script called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

#DEBHELPER#

exit 0
